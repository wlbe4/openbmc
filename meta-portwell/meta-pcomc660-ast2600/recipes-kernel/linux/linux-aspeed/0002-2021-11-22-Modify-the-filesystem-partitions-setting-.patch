From 73425bf38f60ff8655dc1122ea25f51f3aa50964 Mon Sep 17 00:00:00 2001
From: nicklu <nick.lu@portwell.com.tw>
Date: Mon, 22 Nov 2021 15:41:28 +0800
Subject: [PATCH] 2021/11/22 Modify the filesystem partitions setting in the
 kernel device tree.

---
 arch/arm/boot/dts/pcomc660-ast2600.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/pcomc660-ast2600.dts b/arch/arm/boot/dts/pcomc660-ast2600.dts
index 06df6c3099f6..847a60ca42ee 100644
--- a/arch/arm/boot/dts/pcomc660-ast2600.dts
+++ b/arch/arm/boot/dts/pcomc660-ast2600.dts
@@ -235,22 +235,22 @@
 			};
 
 			u-boot-env@e0000 {
-				reg = <0xe0000 0x20000>; // 128KB
+				reg = <0xe0000 0x20000>; 
 				label = "u-boot-env";
 			};
 
 			kernel@100000 {
-				reg = <0x100000 0x600000>; // 9MB
+				reg = <0x100000 0x600000>; 
 				label = "kernel";
 			};
 
 			rofs@600000 {
-				reg = <0x600000 0x1D00000>; // 32MB
+				reg = <0x600000 0x1D00000>; 
 				label = "rofs";
 			};
 
 			rwfs@1D00000 {
-				reg = <0x1D00000 0x300000>; // 22MB
+				reg = <0x1D00000 0x3E30000>; 
 				label = "rwfs";
 			};
 		};
